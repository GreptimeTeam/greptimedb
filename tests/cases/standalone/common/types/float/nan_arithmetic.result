-- FLOAT type
-- any arithmetic on a nan value will result in a nan value
SELECT 'inf'::FLOAT + 1;

+------------------------+
| Utf8("inf") + Int64(1) |
+------------------------+
| inf                    |
+------------------------+

-- inf + inf = inf
SELECT 'inf'::FLOAT + 'inf'::FLOAT;

+---------------------------+
| Utf8("inf") + Utf8("inf") |
+---------------------------+
| inf                       |
+---------------------------+

SELECT 'inf'::FLOAT - 1;

+------------------------+
| Utf8("inf") - Int64(1) |
+------------------------+
| inf                    |
+------------------------+

-- inf - inf = nan
SELECT 'inf'::FLOAT - 'inf'::FLOAT;

+---------------------------+
| Utf8("inf") - Utf8("inf") |
+---------------------------+
| NaN                       |
+---------------------------+

SELECT 'inf'::FLOAT * 1;

+------------------------+
| Utf8("inf") * Int64(1) |
+------------------------+
| inf                    |
+------------------------+

-- inf * inf = inf
SELECT 'inf'::FLOAT * 'inf'::FLOAT;

+---------------------------+
| Utf8("inf") * Utf8("inf") |
+---------------------------+
| inf                       |
+---------------------------+

-- inf * -inf = -inf
SELECT 'inf'::FLOAT * '-inf'::FLOAT;

+----------------------------+
| Utf8("inf") * Utf8("-inf") |
+----------------------------+
| -inf                       |
+----------------------------+

SELECT 'inf'::FLOAT / 1;

+------------------------+
| Utf8("inf") / Int64(1) |
+------------------------+
| inf                    |
+------------------------+

SELECT 1 / 'inf'::FLOAT;

+------------------------+
| Int64(1) / Utf8("inf") |
+------------------------+
| 0.0                    |
+------------------------+

SELECT 'inf'::FLOAT % 1;

+------------------------+
| Utf8("inf") % Int64(1) |
+------------------------+
| NaN                    |
+------------------------+

SELECT 1 % 'inf'::FLOAT;

+------------------------+
| Int64(1) % Utf8("inf") |
+------------------------+
| 1.0                    |
+------------------------+


SELECT v, vec_dim(v)
FROM (
         SELECT '[1.0, 2.0, 3.0]' AS v
         UNION ALL
         SELECT '[-1.0]' AS v
         UNION ALL
         SELECT '[4.0, 5.0, 6.0]' AS v
     ) Order By vec_dim(v) ASC;


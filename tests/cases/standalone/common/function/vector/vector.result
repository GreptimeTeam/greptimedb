SELECT v, vec_kth_elem(v, 0) AS first_elem
FROM (
         SELECT '[1.0, 2.0, 3.0]' AS v
         UNION ALL
         SELECT '[4.0, 5.0, 6.0, 7.0]' AS v
         UNION ALL
         SELECT '[8.0]' AS v
     )
WHERE vec_kth_elem(v, 0) > 2.0 ORDER BY first_elem;

+----------------------+------------+
| v                    | first_elem |
+----------------------+------------+
| [4.0, 5.0, 6.0, 7.0] | 4.0        |
| [8.0]                | 8.0        |
+----------------------+------------+


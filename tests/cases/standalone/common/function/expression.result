SELECT ISNULL(null);

+-------------------------+
| isnull(NULL)            |
+-------------------------+
| true                    |
+-------------------------+

SELECT ISNULL(1);

+-------------------------+
| isnull(1)               |
+-------------------------+
| false                   |
+-------------------------+

SELECT ISNULL(-1);

+-------------------------+
| isnull(-1)              |
+-------------------------+
| false                   |
+-------------------------+

SELECT ISNULL(1.0);

+-------------------------+
| isnull(1.0)             |
+-------------------------+
| false                   |
+-------------------------+

SELECT ISNULL(true);

+-------------------------+
| isnull(true)            |
+-------------------------+
| false                   |
+-------------------------+

SELECT ISNULL('string');

+-------------------------+
| isnull(Utf8("string"))  |
+-------------------------+
| false                   |
+-------------------------+
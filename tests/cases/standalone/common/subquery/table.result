-- table subquery
CREATE TABLE test (ts TIMESTAMP TIME INDEX, i INTEGER, j INTEGER);

Affected Rows: 0

INSERT INTO test VALUES (0, 3, 4), (1, 4, 5), (2, 5, 6);

Affected Rows: 3

SELECT * FROM (SELECT i, j AS d FROM test ORDER BY i) AS b;

